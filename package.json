{
  "name": "MobileWdioRepro",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "adb:reverse": "adb reverse tcp:9900 tcp:9900",
    "android": "react-native run-android",
    "android:apk": "cd android && ./gradlew assembleRelease && cd ../",
    "clean:all": "yarn clean:android && yarn clean:ios && yarn clean:javascript",
    "clean:android": "cd android && ./gradlew clean && cd ../",
    "clean:ios": "rm -rf ~/Library/Developer/Xcode/DerivedData && rm -rf ios/Pods",
    "clean:javascript": "watchman watch-del-all && rm -rf node_modules && rm -rf $TMPDIR/react-* && rm -rf $TMPDIR/haste-* && rm -rf $TMPDIR/metro-*",
    "install:full": "yarn && jetify && yarn pod:install",
    "install:full-debug": "yarn && jetify && yarn pod:install-debug",
    "ios": "react-native run-ios",
    "ios:install-bundles": "bundle install",
    "jetify": "jetify",
    "pod:install": "cd ios/ && pod repo update && export NO_FLIPPER=1 && pod install && unset NO_FLIPPER && cd ../",
    "pod:install-debug": "cd ios/ && pod repo update && pod install && cd ../",
    "pod:update": "cd ios/ && pod repo remove trunk && pod update && cd ../",
    "start": "react-native start",
    "start:clear-cache": "yarn start --reset-cache",
    "xcode": "open ios/MobileWdioRepro.xcworkspace",
    "test:android-sauce": "test/test-execution-scripts/android-saucelabs.sh",
    "test:ios-sauce": "test/test-execution-scripts/ios-saucelabs.sh"
  },
  "dependencies": {
    "axios": "1.4.0",
    "react": "18.2.0",
    "react-native": "0.72.4"
  },
  "devDependencies": {
    "@babel/core": "7.22.9",
    "@babel/eslint-parser": "7.22.9",
    "@babel/plugin-proposal-private-methods": "7.18.6",
    "@babel/plugin-transform-class-properties": "7.22.5",
    "@babel/plugin-transform-private-property-in-object": "7.22.5",
    "@babel/plugin-transform-runtime": "7.11.5",
    "@babel/preset-env": "7.22.9",
    "@babel/register": "7.21.0",
    "@babel/runtime": "7.20.0",
    "@react-native/eslint-config": "0.72.2",
    "@react-native/metro-config": "0.72.11",
    "@rpii/wdio-report-events": "0.2.0",
    "@tsconfig/react-native": "3.0.0",
    "@types/react": "18.0.24",
    "@types/react-test-renderer": "18.0.0",
    "@wdio/appium-service": "8.11.2",
    "@wdio/cli": "8.9.0",
    "@wdio/devtools-service": "8.9.0",
    "@wdio/local-runner": "8.9.0",
    "@wdio/mocha-framework": "8.8.7",
    "@wdio/sauce-service": "8.11.2",
    "@wdio/spec-reporter": "8.11.2",
    "babel-jest": "29.2.1",
    "babel-plugin-macros": "2.8.0",
    "babel-plugin-module-resolver": "5.0.0",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "eslint": "8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-babel-module": "5.3.2",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-react-native": "4.0.0",
    "eslint-plugin-react-native-a11y": "3.3.0",
    "jest": "29.2.1",
    "jetifier": "2.0.0",
    "log4js": "6.9.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "2.8.8",
    "prettier-eslint": "15.0.1",
    "react-test-renderer": "18.2.0",
    "ts-node": "10.9.1",
    "typescript": "4.8.4",
    "wdio-chromedriver-service": "8.1.1",
    "wdio-html-nice-reporter": "8.0.6",
    "wdio-image-comparison-service": "3.0.0",
    "wdio-intercept-service": "4.4.0",
    "wdio-json-reporter": "3.0.0",
    "wdio-mochawesome-reporter": "4.0.0",
    "wdio-native-app-compare-service": "2.1.0",
    "wdio-ocr-service": "1.1.2",
    "wdio-video-reporter": "3.5.0"
  },
  "engines": {
    "node": ">=16"
  }
}
