{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2016",
    "allowJs": true,
    "jsx": "react",
    "moduleResolution": "node",
    "isolatedModules": false,
    "baseUrl": ".",
    "paths": {
      "/*": ["./src/*"]
    },
    "types": ["node", "react", "@wdio/globals/types"],
  },
  "exclude": ["node_modules", "**/node_modules/*"]
}
